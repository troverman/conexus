ht: 1.5em; margin: 15px 0;}blockquote span{font-weight: 600; padding-right: 5px;}ul.getting-started{padding: 25px 75px 25px 0; width: 70%; float: left; -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;}ul.getting-started li{padding: 25px 0;}ul.getting-started li h3 {padding-bottom: 10px; font-size: 25px; font-weight: 300;}div.default-page{min-width: 1200px;}.pocket{display:none;}.main hr {padding-top: 50px; margin-bottom: 50px; border-left: none; border-right: none; border-top: none; border-bottom: 1px solid #ebebeb;}.main h3 {margin-bottom: 15px;}.main ul li{display: inline-block; float: left; margin-right: 25px;}
